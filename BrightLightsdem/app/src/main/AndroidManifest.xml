        <activity android:name=".InstructPg2"></activity>
        <activity android:name=".instructPg3"></activity>
        <activity android:name=".MakeLevelMenuClass"></activity>
        <activity android:name=".MultiMenuClass"></activity>
        <activity android:name=".SixteenLightModeClass"></activity>
        <activity android:name=".LevelMenuClass"></activity>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>