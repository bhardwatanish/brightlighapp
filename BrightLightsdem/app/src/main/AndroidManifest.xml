        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name=".instructPg"></activity>
        <activity android:name=".TwentyFiveLights" ></activity>
        <activity android:name=".LogInClass" ></activity>
        <activity android:name=".MultiPlayerActivity" ></activity>
